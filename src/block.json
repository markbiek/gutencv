{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/gutencv",
	"version": "0.1.0",
	"title": "GutenCV",
	"category": "text",
	"icon": "flag",
	"description": "A Gutenberg block to build CV/Resume entries.",
	"attributes": {
		"company": {
			"type": "string",
			"source": "text",
			"selector": "div"
		},
		"title": {
			"type": "string",
			"source": "text",
			"selector": "div"
		},
		"start_date": {
			"type": "string",
			"source": "text",
			"selector": "div"
		},
		"end_date": {
			"type": "string",
			"source": "text",
			"selector": "div"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "gutencv",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}